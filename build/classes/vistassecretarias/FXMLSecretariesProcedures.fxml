      <Button fx:id="btnLogout" layoutX="37.0" layoutY="514.0" mnemonicParsing="false" onAction="#clicBtnLogout" prefHeight="25.0" prefWidth="91.0" style="-fx-background-color: dimgray;" text="Salir" textFill="WHITE">
